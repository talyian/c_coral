c_extern printf: Func[Ptr, ..., Void]

type Person = {age:Int32}
func Person.getType(): "Person"
func Person.getAge(): self.age

func main():
  let robert = Person(3)
  # calling a member of type Func[Ptr]
  let f = robert.getType()
  # calling a member of type BoundMethod[Int32]
  let g = robert.getAge()
  # calling a class member of Person
  let h = Person.getType()
  printf("%s - %s - %d\n", f, h, g)
  0
